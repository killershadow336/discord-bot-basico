# Configuración del Bot de Discord
# Reemplaza estos valores con tus credenciales reales del bot

# Tu token del bot desde el Portal de Desarrolladores de Discord
BOT_TOKEN=tu_token_del_bot_aqui

# El client ID de tu bot
CLIENT_ID=tu_client_id_aqui

# IDs de servidores donde se registrarán los comandos (separados por comas para múltiples servidores)
# Déjalo vacío para comandos globales (puede tomar hasta 1 hora en actualizarse)
GUILD_IDS=id_servidor_1,id_servidor_2

# Opcional: ID de usuario del propietario para funciones de administrador
OWNER_ID=tu_user_id_aqui 